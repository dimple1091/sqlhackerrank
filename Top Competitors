select s.hacker_id,h.name from submissions s join challenges c on s.challenge_id=c.challenge_id  join difficulty d on d.difficulty_level=c.difficulty_level and s.score=d.score join hackers h on h.hacker_id=s.hacker_id group by s.hacker_id,name having count(s.challenge_id)>1 order by count(s.challenge_id) desc,hacker_id;
